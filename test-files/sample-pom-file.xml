		<maven.compiler.source>11</maven.compiler.source>
		<maven.compiler.target>11</maven.compiler.target>
		<web3j-quorum.version>4.8.4</web3j-quorum.version>
		<commons-lang3.version>3.11</commons-lang3.version>
